{
    "version": 2,
    "rewrites": [
        {
            "source": "/",
            "destination": "/main/index.html"
        },
        {
            "source": "/works",
            "destination": "/works/index.html"
        },
        {
            "source": "/works/:path*",
            "destination": "/works/:path*"
        }
    ],
    "cleanUrls": true
}