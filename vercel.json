{
    "version": 2,
    "rewrites": [
    { "source": "/works/(.*)", "destination": "/works/index.html" }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/main/index.html"
        },
        {
            "src": "/works",
            "dest": "/works/index.html"
        },
        {
            "src": "/works/(.*)",
            "dest": "/works/$1"
        }
    ]
}


    

